<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<display-name>RegistrationServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.RegistrationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewProfileServlet</servlet-name>
  	<display-name>ViewProfileServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.ViewProfileServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmailValidationServlet</servlet-name>
  	<display-name>EmailValidationServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.EmailValidationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoadCountryServlet</servlet-name>
  	<display-name>LoadCountryServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.LoadCountryServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoadCaptchaServlet</servlet-name>
  	<display-name>LoadCaptchaServlet</display-name>
  	<description></description>
  	<servlet-class>com.facebookweb.controller.LoadCaptchaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegistrationServlet</servlet-name>
  	<url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewProfileServlet</servlet-name>
  	<url-pattern>/ViewProfileServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmailValidationServlet</servlet-name>
  	<url-pattern>/EmailValidationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoadCountryServlet</servlet-name>
  	<url-pattern>/LoadCountryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoadCaptchaServlet</servlet-name>
  	<url-pattern>/LoadCaptchaServlet</url-pattern>
  </servlet-mapping>
</web-app>
